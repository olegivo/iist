<UserControl
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	xmlns:local="clr-namespace:UICommonWPF"
	mc:Ignorable="d"
	x:Class="UICommonWPF.ucChamber"
	x:Name="UserControl"
	d:DesignWidth="640" d:DesignHeight="480" Width="320" Height="320">

	<Grid x:Name="LayoutRoot">
		<Viewbox Stretch="Fill">
			<Grid Height="320" Width="320">
				<Rectangle Stroke="#FF00458B" StrokeThickness="5"/>
				<local:UserControl1 HorizontalAlignment="Right" Margin="0,0,80,8" RenderTransformOrigin="0.5,0.5" Width="48" Height="80" VerticalAlignment="Bottom"/>
				<local:UserControl1 HorizontalAlignment="Left" Margin="80,0,0,8" RenderTransformOrigin="0.5,0.5" Width="48" VerticalAlignment="Bottom" Height="80"/>
				<Rectangle Height="120" Stroke="#FF00458B" StrokeThickness="5" VerticalAlignment="Top"/>
			</Grid>
		</Viewbox>
	</Grid>
</UserControl>