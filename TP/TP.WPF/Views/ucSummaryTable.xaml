<UserControl
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
    xmlns:dg="clr-namespace:Microsoft.Windows.Controls;assembly=WPFToolkit" mc:Ignorable="d"
	x:Class="TP.WPF.Views.ucSummaryTable"
	x:Name="UserControl"
	d:DesignWidth="640" d:DesignHeight="480" Width="1024" Height="768">
    <Grid>
        <dg:DataGrid  Name="grid" ItemsSource="{Binding SummaryTable}" IsReadOnly="True" Width="Auto" />
    </Grid>
</UserControl>
